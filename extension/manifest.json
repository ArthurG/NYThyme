{
  "manifest_version": 2,

  "name": "NYThyme",
  "description": "Chrome extension to scrapes articles on NYTimes",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.nytimes.com/*"],
      "js": ["js/jquery.js", "js/nyt.js"]
    }
  ],
  "permissions": [
    "tabs", "https://nytimes.com/"
  ]
}
